<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd"    default-autowire="byName" default-lazy-init="false">
	
	<!-- rpc service -->
	<bean id="cityWebService" class="com.company.project.webservice.impl.CityWebServiceImpl"/>
	
	<!-- 调用示例: http://localhost:6060/rpc/CityWebService/getById  -->
	<bean class="com.github.rapid.common.rpc.server.RPCServiceExporter">
	  <property name="serviceInterface" value="com.company.project.webservice.CityWebService" />
	  <property name="service" ref="cityWebService"/>
	</bean>

</beans>
